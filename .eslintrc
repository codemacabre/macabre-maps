{
  "env": {
    "es2021": true,
    "jest/globals": true,
    "node": true,
    "browser": true
  },
  "plugins":[
    "jest",
    "react"
  ],
  "extends": [
    "standard",
    "standard-jsx",
    "standard-react",
    "eslint:recommended",
